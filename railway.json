{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "dockerfile"
  },
  "deploy": {
    "numReplicas": 1,
    "startCommand": "python -m uvicorn app.main:app --host 0.0.0.0 --port $PORT"
  },
  "plugins": [
    "postgresql"
  ],
  "variables": {
    "API_KEY": "dev-key",
    "CORS_ORIGINS": "*"
  }
}
